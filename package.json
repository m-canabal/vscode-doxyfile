{
    "name": "vscode-doxyfile",
    "displayName": "vscode-doxyfile",
    "description": "Doxyfile language support",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.74.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "doxyfile",
                "aliases": [
                    "Doxyfile",
                    "doxyfile"
                ],
                "extensions": [
                    "Doxyfile"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "doxyfile",
                "scopeName": "source.doxyfile",
                "path": "./syntaxes/doxyfile.tmLanguage.json"
            }
        ]
    }
}